                                          <Insets bottom="10.0" />
                                       </VBox.margin></Button>
                                    <Button mnemonicParsing="false" prefHeight="43.0" prefWidth="148.0" text="UPDATE TASK" />
                                 </children>
                              </VBox>
                           </children>
                        </AnchorPane>
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab text="Training">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="571.0" prefWidth="700.0">

                     </children></AnchorPane>
            </content>
          </Tab>
          <Tab text="Tasks ">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <AnchorPane prefHeight="571.0" prefWidth="700.0">
                           <children>
                              <VBox layoutX="28.0" layoutY="38.0" prefHeight="410.0" prefWidth="146.0">
                                 <children>
                                    <Button mnemonicParsing="false" onAction="#openAddTaskWindow" prefHeight="43.0" prefWidth="148.0" text="ADD TASK">

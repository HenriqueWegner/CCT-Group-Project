                                    <Insets bottom="10.0" />
                                 </VBox.margin>
                              </Button>
                              <Button mnemonicParsing="false" onAction="#searchEmployee" prefHeight="43.0" prefWidth="148.0" text="SEARCH EMPLOYEE">
